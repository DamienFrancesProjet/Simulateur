{% extends "::container.html.twig" %}

{% block body %}
    <div class="container">
        <div class="row">
            <h1>Gestion des résultats</h1>
            <div class="row">
                <div class="col-xs-5 col-xs-offset-1">
                    <label>Supprimer les résultats datant de plus d'un an</label>
                    <div><a href="{{ path('fd_result_remove') }}" class="btn btn-danger active">Supprimer</a></div>
                </div>
                <div class="col-xs-5 col-xs-offset-1">
                    <label>Récupérer les résultats de la veille de type 1/N/2</label>
                    <div><a href="{{ path('fd_result_get') }}" class="btn btn-primary active">Récupérer</a></div>
                </div>
            </div>
            {% block response %}
            {% endblock %}
        </div>
    </div>
{% endblock %}
